{
  "expo": {
    "name": "Cinema Club",
    "slug": "cinemaclub",
    "githubUrl": "https://github.com/rodrinac/cinemaclub",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "newArchEnabled": true,
    "android": {
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.nu.production",
      "package": "com.anonymous.cinemaclub"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.anonymous.cinemaclub"
    },
    "plugins": [
      "expo-secure-store",
      "expo-sqlite",
      "expo-localization",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#E3E8F5",
          "image": "./assets/splash.png",
          "dark": {
            "image": "./assets/splash.png",
            "backgroundColor": "#1c2646"
          },
          "imageWidth": 216
        }
      ],
      "expo-localization"
    ]
  }
}
